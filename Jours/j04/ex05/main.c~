void	ft_putnbr(int nb);

void	ft_putchar(char c);

int		ft_sqrt(int nb);

int		main(void)
{
	ft_putnbr(ft_sqrt(1));
	ft_putchar('\n');
	ft_putnbr(ft_sqrt(-6));
	ft_putchar('\n');
	ft_putnbr(ft_sqrt(9));
	ft_putchar('\n');
	ft_putnbr(ft_sqrt(8));
	return (0);
}
