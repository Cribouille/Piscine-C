int		main(void)
{
	ft_putnbr(ft_find_next_prime(0));
	ft_putchar('\n');
	ft_putnbr(ft_find_next_prime(5));
	ft_putchar('\n');
	ft_putnbr(ft_find_next_prime(8));
	ft_putchar('\n');
	ft_putnbr(ft_find_next_prime(20));
	ft_putchar('\n');
	ft_putnbr(ft_find_next_prime(-7));
	ft_putchar('\n');
	ft_putnbr(ft_find_next_prime(9));
	return (0);
}
