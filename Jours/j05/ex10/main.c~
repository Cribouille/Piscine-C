char	*ft_strcapitalize(char *str);

void	ft_putstr(char *str);

int		main(void)
{
	char str[100] = "salut, comMent tu vas ? 42mots quarante-deux; cinquante+et+un";
	ft_putstr(ft_strcapitalize(str));
	return (0);
}
