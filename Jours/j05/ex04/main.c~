void	ft_putstr(char *str);

char	*ft_strncpy(char *dest, char *src, unsigned int n);

int		main(void)
{
	char	str[8] = "Etienne";
	char	str2[6] = "Tinou";
	ft_putstr(ft_strncpy(str, str2, 8));
	return (0);
}
