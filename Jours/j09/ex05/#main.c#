int		ft_button(int i, int j,int k);

void	ft_putchar(char c);

void	ft_putnbr(int nb);

int		main()
{
	ft_putnbr(ft_button(4, 8, 9));
	ft_putchar('\n');
	ft_putnbr(ft_button(4, 2, -4));
	ft_putchar('\n');
	ft_putnbr(ft_button(4, -8, -8));
	return (0);
}
