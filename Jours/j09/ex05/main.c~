int		ft_button(int i, int j,int j);

int		main()
{
	ft_button(4, 8, 9);
	ft_putchar('\n');
	ft_button(4, 8, 9);
	ft_putchar('\n');
	ft_button(4, 8, 9);
	return (0);
}
