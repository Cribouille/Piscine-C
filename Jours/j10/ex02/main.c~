int		divise_number(int nb)
{
	return (nb / 2);
}

int		*ft_map(int *tab, int length, int (*f)(int));

int		main()
{
	int		*tab[6] = {4, 6, 2, 7, 13, 3};
	ft_map(tab, 6, &divise_number);
	return (0);
}
