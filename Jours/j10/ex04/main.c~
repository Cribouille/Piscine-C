int		ft_count_if(char **tab, int (*f)(char*));

int		len_min_7(char *str)
{
	int		len;

	len = 0;
	while (str[i])
		len++;
	if (len >= 7)
		return (1);
	return (0);
}


void	ft_putnbr(int nb);

int		main()
{
	int		*tab[9] = {"etienne", "hello", "coucou", 0};	

	ft_putnbr(ft_count_if(tab, &len_min_7));
