int		find_e(char *str)
{
	int		i;

	i = 0;
	while (str[i])
	{
		if (str[i] == 'e')
			return (1);
		i++;
	}
	return (0);
}

int		main()
{
	char	**tab = {"hello", "Etienne"};

	ft_any(tab, &find_e);
	return (0);
}
